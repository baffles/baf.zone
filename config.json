{
	"locals": {
		"url": "http://baf.zone",
		"name": "BAFZone",
		"footerName": "Robert Ferris"
	},
	"plugins": [
		"./plugins/blog.coffee",
		"wintersmith-stylus",
		"wintersmith-coffee"
	],
	"require": {
		"moment": "moment",
		"_": "underscore",
		"typogr": "typogr"
	},
	"jade": {
		"pretty": true
	},
	"markdown": {
		"smartLists": true,
		"smartypants": true
	},
	"blog": {
		"pageTemplate": "blog-listing.jade",
		"pageDynamicTemplate": "blog-page-teasers.jade",
		"permalinkTemplate": "tumblr-permalink.jade",
		"posts": "blog/posts",
		"first": "blog/index.html",
		"page": "blog/page/%d/index.html",
		"pageJson": "blog/page/%d.json",
		"perPage": 5,
		"tumblrBase": "http://blog.baf.cc/"
	}
}
